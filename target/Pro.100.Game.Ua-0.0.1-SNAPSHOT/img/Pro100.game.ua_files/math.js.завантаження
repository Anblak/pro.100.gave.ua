//баттони вибудовують стрінгу
var input = "null";
function zero() {
	var div = document.getElementById(input);
	div.value += "0";
}

function one() {
	var div = document.getElementById(input);
	div.value += "1";
}

function two() {
	var div = document.getElementById(input);
	div.value += "2";
}

function three() {
	var div = document.getElementById(input);
	div.value += "3";
}

function four() {
	var div = document.getElementById(input);
	div.value += "4";
}

function five() {
	var div = document.getElementById(input);
	div.value += "5";
}

function six() {
	var div = document.getElementById(input);
	div.value += "6";
}

function seven() {
	var div = document.getElementById(input);
	div.value += "7";
}

function eight() {
	var div = document.getElementById(input);
	div.value += "8";
}

function nine() {
	var div = document.getElementById(input);
	div.value += "9";
}

function alt() {
	var div = document.getElementById(input);
	div.value += "@";
}
function association() {
	var div = document.getElementById(input);
	div.value += "/";
}
function odds() {
	var div = document.getElementById(input);
	div.value += "+";
}
function decussation() {
	var div = document.getElementById(input);
	div.value += "^";
}
function coma() {
	var div = document.getElementById(input);
	div.value += "(";
}
function brackets() {
	var div = document.getElementById(input);
	div.value += ")";
}
function inputSpace() {
	input = "space";
}
function inputFormul() {
	input = "formul";
}
